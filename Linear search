int linear(int n,int a[],int s,int t)
{
    for(int i=0;i<n;i++)
    {
        if(a[i]==s)
        {
            t=i+1;
            break;
        }
    }
    return t;
}

void input(int n,int a[])
{
    printf("Enter elements of array:");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
}

void output(int t)
{
    printf("Position=%d\n",t);
}

void main()
{
    int n,t,s;
    printf("Enter number of elements:");
    scanf("%d",&n);
    int a[n];
    input(n,a);
    printf("Enter number to be searched:");
    scanf("%d",&s);
    int b=linear(n,a,s,t);
    output(b);
}
