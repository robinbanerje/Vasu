int linear(int a[],int n,int s)
{
    int t=0;
    for(int i=0;i<n;i++)
    {
        if(a[i]==s)
        {
            t++;
        }
    }
    return t;
}

void main()
{
    int a[100],n,s;
    printf("Enter number of elements:");
    scanf("%d",&n);
    printf("Enter elements of array:");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Ente number to be searched:");
    scanf("%d",&s);
    int t=linear(a,n,s);
    printf("Occurence:%d",t);
}
