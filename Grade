#include<stdio.h>
char grade(int m)
{
    char c;
    if(m>=90&&m<=100)
        c='A';
    else if(m>=80&&m<90)
        c='B';
    else if(m>=70&&m<80)
        c='C';
    else if(m>=60&&m<70)
        c='D';
    else if(m>=50&&m<60)
        c='E';
    else if(m<50)
        c='F';
    return c;
}

void main()
{
    int m;
    printf("Enter marks:");
    scanf("%d",&m);
    char ch=grade(m);
    printf("Grade=%c",ch);
}
