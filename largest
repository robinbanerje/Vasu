#include <stdio.h>
float largest(float a,float b, float c)
{
    float l;
    if(a>b&&a>c)
    {
    l=a;}
    else if(b>a&&b>c)
   { l=b;}
    else if(c>a&&c>b)
    { l=c;}
    printf("largest=%f",l);
}
int main()
{
float x,y,z;
printf("Enter 3 numbers:");
scanf("%f%f%f",&x,&y,&z);
largest(x,y,z);
return 0;
}
